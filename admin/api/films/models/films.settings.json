{
  "kind": "collectionType",
  "collectionName": "films",
  "info": {
    "name": "Films",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "deleted": {
      "type": "boolean"
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "reacts": {
      "plugin": "users-permissions",
      "collection": "user"
    },
    "views": {
      "type": "biginteger"
    }
  }
}
